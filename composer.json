{
    "name": "gwissem/legacy-wrapper-bundle",
    "type": "symfony-bundle",
    "keywords": ["legacy", "symfony1", "symfony2"],
    "homepage": "http://www.theodo.fr",
    "license": "MIT",
    "authors": [
        {
            "name": "Theodo",
            "homepage": "http://www.theodo.fr"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Theodo\\Evolution\\Bundle\\LegacyWrapperBundle\\": ""
